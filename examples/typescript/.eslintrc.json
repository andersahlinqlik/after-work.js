{
  "env": {
    "amd": true,
    "browser": true
  },
  "globals": {
    "requirejs": true
  },
  "rules": {
    "import/no-amd": 0,
    "prefer-arrow-callback": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "no-var": 0
  },
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script"
  }
}
